{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": [
    {
      "site": "sfscioly",
      "public": "sfscioly",
      "cleanUrls": true,
      "redirects": [
        {
          "source": "/join",
          "destination": "/index.html#join",
          "type": 302
        },
        {
          "source": "/at",
          "destination": "/attendance.html?date=00000000",
          "type": 302
        }
      ],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "site": "codetaker",
      "public": "codetaker",
      "cleanUrls": true,
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}